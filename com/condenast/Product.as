package com.condenast{	import flash.display.Sprite;	import com.greensock.TimelineLite;	import com.greensock.TweenMax;	import com.greensock.easing.Quint;	import flash.events.MouseEvent;		public class Product extends Sprite {		private var _lib:Object;		private var _track:Function;				public function Product($lib:Object, $f:Function) {			_lib = $lib;			_track = $f;			init();		}		private function init():void{			//Your code goes here!			var hit:Sprite = new Sprite;			hit.graphics.beginFill(0x00d9f9, 0);			hit.graphics.drawRect(0, 0, 970, 90);			hit.graphics.endFill();			addChild(_lib.image0);			if(_lib.image1 && _lib.image2){				addChild(_lib.image1);				addChild(_lib.image2);			}			addChild(_lib.header);			addChild(hit);			var t:TimelineLite = new TimelineLite;			t.insert(TweenMax.from(_lib.header, .5, {x:-370, ease:Quint.easeOut}));			t.insert(TweenMax.to(_lib.image0, .5, {startAt:{x:970}, x:370, ease:Quint.easeOut, immediateRender:true}));			if(_lib.image1 && _lib.image2){				t.insert(TweenMax.to(_lib.image0, .5, {x:-230, ease:Quint.easeOut}), 3);				t.insert(TweenMax.to(_lib.image1, .5, {startAt:{x:970}, x:370, ease:Quint.easeOut, immediateRender:true}), 3);				t.insert(TweenMax.to(_lib.image1, .5, {x:-230, ease:Quint.easeOut}), 6);				t.insert(TweenMax.to(_lib.image2, .5, {startAt:{x:970}, x:370, ease:Quint.easeOut, immediateRender:true}), 6);				t.insert(TweenMax.to(_lib.image2, .5, {x:-230, ease:Quint.easeOut}), 9);				t.insert(TweenMax.to(_lib.image0, .5, {startAt:{x:970}, x:370, ease:Quint.easeOut}), 9);				t.insert(TweenMax.to(_lib.image0, .5, {x:-230, ease:Quint.easeOut}), 12);				t.insert(TweenMax.to(_lib.image1, .5, {startAt:{x:970}, x:370, ease:Quint.easeOut}), 12);				t.insert(TweenMax.to(_lib.image1, .5, {x:-230, ease:Quint.easeOut}), 15);				t.insert(TweenMax.to(_lib.image2, .5, {startAt:{x:970}, x:370, ease:Quint.easeOut}), 15);			}			hit.buttonMode = true;			hit.addEventListener(MouseEvent.MOUSE_DOWN, function():void{ _track('click', 'Nav - Expand'); _track('message', 'expand');});		}	}	}